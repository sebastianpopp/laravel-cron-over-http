{
    "name": "sebastianpopp/laravel-cron-over-http",
    "description": "Run your Laravel cron jobs and queue over HTTP.",
    "keywords": [
        "Sebastian Popp",
        "laravel",
        "laravel-cron-over-http"
    ],
    "homepage": "https://github.com/sebastianpopp/laravel-cron-over-http",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Popp",
            "email": "git@sebastianpopp.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SebastianPopp\\CronOverHttp\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SebastianPopp\\CronOverHttp\\CronOverHttpServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
